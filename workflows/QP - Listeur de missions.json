    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "let json = $json;\n\nif (json.json) {\n json = {\n   ...json,\n   ...JSON.parse(json.json)\n }; \n}\n\nif (json.chatInput) {\n json.query = json.chatInput;\n}\n\nif (!json.query) {\n  json.query = \"Je souhaite un point général sur la situation de la production.\";\n}\n\n// Trouver le bon Kin\nlet regexKin = /kin\\w{2,16}/i;\nlet regexKinWithSpace = /kin \\w{2,16}/i;\n\nif (regexKin.test(json.query)) {\n  json.kin = json.query.match(regexKin)[0];\n} else if (regexKinWithSpace.test(json.query)) {\n  json.kin = json.query.match(regexKinWithSpace)[0].replace(\" \", \"\");\n}\n\nreturn {\"json\": json};"
      },
